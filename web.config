<?xml version="1.0" encoding="UTF-8"?>
<configuration>
<appSettings>
            <add key="NODE_ENV" value="production" />
        </appSettings>
    <system.webServer>
        <directoryBrowse enabled="true" />
	<handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>
	 <rewrite>
	 <rules>
       <clear />
        <rule name="cdw">
          <match url="/*" />
          <action type="Rewrite" url="server.js" />
        </rule>
      </rules>
    </rewrite>
    </system.webServer>
</configuration>

 